{
    "name": "RGI Development",
    "dockerComposeFile": [
        "../docker-compose.yml"
    ],
    "service": "rgi-env",
    "workspaceFolder": "/workspaces/rgi",
    "remoteUser": "ubuntu",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "ms-python.vscode-pylance"
            ],
            "settings": {
                "python.defaultInterpreterPath": "/app/.venv/bin/python",
                "python.analysis.extraPaths": [
                    "/workspaces/rgi"
                ]
            }
        }
    }
}